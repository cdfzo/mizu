{
  "name": "mizu",
  "displayName": "Mizu Icons",
  "description": "Mizu Icons for Visual Studio Code",
  "version": "2.0.0",
  "license": "MIT",
  "publisher": "cdfzo",
  "homepage": "https://codeberg.org/cdfzo/mizu",
  "icon": "mizu.png",
  "main": "extension.js",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icons",
    "material",
    "theme",
    "dark"
  ],
  "scripts": {
    "dev": "bun run build:ext --watch",
    "lint": "bunx --bun eslint src tests",
    "test": "bun test",
    "build": "bun run src/dev/build.ts",
    "build:ext": "bunx --bun esbuild src/extension.ts --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --bundle --minify"
  },
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/cdfzo/mizu.git"
  },
  "bugs": {
    "url": "https://codeberg.org/cdfzo/mizu/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "mizu",
        "label": "Mizu",
        "path": "icon-theme.json"
      }
    ],
    "configuration": {
      "title": "Mizu",
      "properties": {
        "mizu.customIconAssociations": {
          "type": "object",
          "description": "Define custom icon associations. This will be released in a future version. Do not use it yet.",
          "properties": {
            "icon": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "devDependencies": {
    "@cdfzo/eslint-config": "^1.0.0",
    "@types/bun": "^1.1.0",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.26.0",
    "esbuild": "^0.21.0",
    "svgo": "^3.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
