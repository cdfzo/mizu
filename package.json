{
  "name": "mizu",
  "displayName": "Mizu Icons",
  "description": "Mizu Icons for Visual Studio Code",
  "version": "1.8.0",
  "license": "MIT",
  "publisher": "cdfzo",
  "homepage": "https://github.com/cdfzo/mizu#readme",
  "icon": "media/mizu.png",
  "main": "extension.js",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icons",
    "material",
    "theme",
    "dark"
  ],
  "scripts": {
    "dev": "bun run build:ext --watch",
    "lint": "bunx --bun eslint .",
    "test": "bun test",
    "build": "bun run src/build.ts",
    "build:ext": "bunx --bun esbuild src/extension.ts --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --bundle --minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdfzo/mizu.git"
  },
  "bugs": {
    "url": "https://github.com/cdfzo/mizu/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:mizu.update"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "mizu",
        "label": "Mizu Icons",
        "path": "icon-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@cdfzo/eslint-config": "^0.1.0",
    "@types/bun": "^1.0.0",
    "@types/vscode": "^1.70.0",
    "@vscode/vsce": "^2.24.0",
    "esbuild": "^0.20.0",
    "svgo": "^3.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
